{
  "name": "@caliopen/frontend",
  "version": "0.20.1",
  "description": "CaliOpen UI is the user interface for CaliOpen.",
  "homepage": "https://caliopen.org/",
  "private": true,
  "engines": {
    "node": ">=8.11.1"
  },
  "scripts": {
    "start": "npm-run-all -l build:clean build:i18n --parallel --race start:dev:server start:dev:assets",
    "start:dev": "HAS_SSR=false npm-run-all -l build:clean build:i18n --parallel --race start:dev:server start:dev:assets",
    "start:dev:server": "CALIOPEN_API_HOSTNAME=localhost CALIOPEN_WEB_SERVER_PORT=4001 CALIOPEN_WEB_SERVER_HOSTNAME=127.0.0.1 bin/dev-server",
    "start:dev:assets": "NODE_ENV=development CALIOPEN_MOTD='credentials: dev / 123456' webpack-dev-server --host 0.0.0.0 --progress --config webpack/webpack.config.browser.js --port 4000",
    "start:prod": "NODE_ENV=production ./bin/server",
    "start:desktop": "npm-run-all --parallel --race build:desktop:dev start:desktop:electron",
    "start:desktop:electron": "NODE_ENV=development electron ./electron/",
    "start:smartphone": "npm-run-all --parallel --race build:smartphone:dev start:smartphone:android",
    "start:smartphone:android": "cd cordova && nodemon --watch www --exec 'cordova emulate android --debug'",
    "start:mock-api": "../../../devtools/api-mock/bin/start",
    "build:clean": "if [ -d dist ];then rm -r dist;fi",
    "build:browser:test": "NODE_ENV=production CALIOPEN_PIWIK_SITE_ID=false webpack -p --config webpack/webpack.config.browser.js",
    "build:server:test": "NODE_ENV=production HAS_SSR=true webpack -p --config webpack/webpack.config.server.js",
    "build:desktop:dev": "NODE_ENV=development webpack --watch --config webpack/webpack.config.electron.js",
    "build:smartphone:dev": "NODE_ENV=development webpack --watch --config webpack/webpack.config.cordova.js",
    "build:i18n": "lingui compile",
    "release": "npm-run-all build:i18n release:web:browser release:web:server",
    "release:web:server": "NODE_ENV=production webpack --bail -p --progress --config webpack/webpack.config.server.js",
    "release:web:browser": "NODE_ENV=production webpack --bail -p --progress --config webpack/webpack.config.browser.js",
    "cordova": "cd cordova && cordova",
    "test": "npm-run-all lint test:unit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:functional": "npm-run-all --sequential -n build:i18n build:browser:test build:server:test test:functional:start",
    "test:functional:start": "npm-run-all -n --parallel --race start:mock-api test:functional:serve test:e2e:delay",
    "test:functional:serve": "NODE_ENV=production CALIOPEN_API_HOSTNAME=localhost ./bin/server",
    "test:e2e": "protractor ./test/functional/protractor.conf.js",
    "test:e2e:debug": "node --inspect-brk node_modules/.bin/protractor ./test/functional/protractor.conf.js",
    "test:e2e:delay": "sleep 3 && protractor ./test/functional/protractor.conf.js",
    "test:e2e:init": "webdriver-manager update",
    "lint": "npm-run-all lint:js lint:styles",
    "lint:js": "eslint --ext .js --ext .jsx --ignore-pattern *.spec.js --ignore-pattern *.spec.jsx src/",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "i18n": "react-translate-scripts",
    "lingui": "NODE_ENV=test lingui"
  },
  "author": "CaliOpen <contact@caliopen.org> (https://caliopen.org/)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@lingui/react": "^2.7.4",
    "argv": "^0.0.2",
    "async-validator": "^1.11.0",
    "axios": "^0.18.0",
    "base64-js": "^1.3.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "diacritics": "^1.3.0",
    "elliptic": "^6.4.1",
    "express": "^4.16.4",
    "express-http-proxy": "1.1.0",
    "font-awesome": "^4.6.3",
    "foundation-sites": "~6.3.0",
    "history": "^4.9.0",
    "iron": "^4.0.5",
    "jquery": "^3.3.1",
    "jssha": "^2.3.1",
    "linkifyjs": "^2.1.8",
    "locale": "^0.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.intersection": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "lodash.throttle": "^4.1.1",
    "mime-types": "^2.1.23",
    "mimemessage": "^1.0.5",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "openpgp": "^4.4.10",
    "piwik-react-router": "^0.12.1",
    "prop-types": "^15.7.2",
    "quoted-printable": "^1.0.1",
    "react": "^16.8.6",
    "react-country-region-selector": "^1.4.3",
    "react-datepicker": "^2.4.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.8.1",
    "react-moment": "^0.9.2",
    "react-redux": "^7.0.2",
    "react-redux-notify": "^4.1.1",
    "react-router-dom": "^5.0.0",
    "react-swipeable": "^5.1.0",
    "react-visibility-sensor": "^5.0.2",
    "reactour": "^1.10.0",
    "redux": "^4.0.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-form": "^8.2.0",
    "reselect": "^4.0.0",
    "scroll": "^3.0.1",
    "serialize-javascript": "^1.7.0",
    "serve-favicon": "^2.5.0",
    "styled-components": "4.2.0",
    "utf8": "^3.0.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-syslog": "^2.1.0",
    "xregexp": "^4.2.4",
    "zxcvbn": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@lingui/cli": "^2.7.4",
    "@lingui/macro": "^2.7.4",
    "autoprefixer": "^9.5.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-macros": "^2.5.1",
    "babel-preset-lingui-react": "^1.0.14",
    "cordova": "^7.1.0",
    "css-loader": "^2.1.1",
    "deep-freeze-strict": "^1.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "geckodriver": "^1.16.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "image-webpack-loader": "^4.6.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest-cli": "^24.7.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^0.1.1",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "protractor": "5.4.2",
    "protractor-console-plugin": "^0.1.1",
    "raf": "^3.4.1",
    "raw-loader": "^2.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^8.0.0",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-order": "^2.2.1",
    "stylelint-scss": "^3.6.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "webpack-pwa-manifest": "^4.0.0",
    "worker-loader": "^2.0.0"
  },
  "lingui": {
    "sourceLocale": "en",
    "fallbackLocale": "en",
    "format": "minimal",
    "srcPathDirs": [
      "<rootDir>/src"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "globals": {
      "CALIOPEN_ENV": "test",
      "BUILD_TARGET": "none",
      "CALIOPEN_OPTIONS": {}
    }
  }
}
